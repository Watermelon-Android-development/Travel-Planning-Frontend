<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"


    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/planList"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FDF5E6">


        <TextView
            android:id="@+id/planA"
            android:layout_width="350dp"
            android:layout_height="117dp"
            android:layout_marginTop="188dp"
            android:background="@drawable/ovaltext"
            android:gravity="center"
            android:text="@string/planSites"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.475"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/Topbackground" />

        <TextView
            android:id="@+id/planB"
            android:layout_width="350dp"
            android:layout_height="117dp"
            android:layout_marginTop="32dp"
            android:background="@drawable/ovaltext"
            android:gravity="center"
            android:text="@string/planSites"
            app:layout_constraintEnd_toEndOf="@+id/planA"
            app:layout_constraintHorizontal_bias="0.524"
            app:layout_constraintStart_toStartOf="@+id/planA"
            app:layout_constraintTop_toBottomOf="@+id/planA" />

        <TextView
            android:id="@+id/planC"
            android:layout_width="350dp"
            android:layout_height="117dp"
            android:layout_marginTop="32dp"
            android:background="@drawable/ovaltext"
            android:gravity="center"
            android:text="@string/planSites"
            app:layout_constraintEnd_toEndOf="@+id/planB"
            app:layout_constraintHorizontal_bias="0.523"
            app:layout_constraintStart_toStartOf="@+id/planB"
            app:layout_constraintTop_toBottomOf="@+id/planB" />

        <ImageButton
            android:id="@+id/del1"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginEnd="24dp"
            android:background="#42ADFB"
            app:layout_constraintBottom_toBottomOf="@+id/planA"
            app:layout_constraintEnd_toEndOf="@+id/planA"
            app:layout_constraintTop_toTopOf="@+id/planA"
            app:srcCompat="@drawable/delete"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/del2"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginEnd="24dp"
            android:background="#42ADFB"
            app:layout_constraintBottom_toBottomOf="@+id/planB"
            app:layout_constraintEnd_toEndOf="@+id/planB"
            app:layout_constraintTop_toTopOf="@+id/planB"
            app:srcCompat="@drawable/delete"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/del3"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginEnd="24dp"
            android:background="#42ADFB"
            app:layout_constraintBottom_toBottomOf="@+id/planC"
            app:layout_constraintEnd_toEndOf="@+id/planC"
            app:layout_constraintTop_toTopOf="@+id/planC"
            app:srcCompat="@drawable/delete"
            tools:ignore="SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/date1"
            android:layout_width="50dp"
            android:layout_height="22dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/ovaltextwhite"
            android:gravity="center"
            android:text="@string/planDates"
            app:layout_constraintStart_toStartOf="@+id/planA"
            app:layout_constraintTop_toTopOf="@+id/planA" />

        <TextView
            android:id="@+id/date2"
            android:layout_width="50dp"
            android:layout_height="22dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/ovaltextwhite"
            android:gravity="center"
            android:text="@string/planDates"
            app:layout_constraintStart_toStartOf="@+id/planB"
            app:layout_constraintTop_toTopOf="@+id/planB" />

        <TextView
            android:id="@+id/date3"
            android:layout_width="50dp"
            android:layout_height="22dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/ovaltextwhite"
            android:gravity="center"
            android:text="@string/planDates"
            app:layout_constraintStart_toStartOf="@+id/planC"
            app:layout_constraintTop_toTopOf="@+id/planC" />

        <ImageView
            android:id="@+id/Topbackground"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="-8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/topBackG" />

        <TextView
            android:id="@+id/planListTitle"
            android:layout_width="180dp"
            android:layout_height="64dp"
            android:layout_marginTop="48dp"
            android:fontFamily="@font/peace_sans"
            android:gravity="center"
            android:text="@string/planTitle"
            android:textColor="#FCF4E5"
            android:textSize="32sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/Topbackground" />

        <ImageView
            android:id="@+id/Bottombackground"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="-8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/bottomBackG" />

        <ImageButton
            android:id="@+id/homepage"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginStart="32dp"
            android:layout_marginBottom="16dp"
            android:background="#5EB9FB"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/home"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/map"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginBottom="16dp"
            android:background="#5EB9FB"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/map"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/profile"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginEnd="32dp"
            android:layout_marginBottom="16dp"
            android:background="#5EB9FB"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:srcCompat="@drawable/profile"
            tools:ignore="SpeakableTextPresentCheck" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.coordinatorlayout.widget.CoordinatorLayout>
